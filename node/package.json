{
  "name": "@cmdop/node",
  "version": "2026.2.28",
  "description": "CMDOP SDK for Node.js - Server-side agent interaction via gRPC",
  "author": "CMDOP <dev@cmdop.com>",
  "homepage": "https://cmdop.com/sdk/node/",
  "documentation": "https://cmdop.com/docs/sdk/node/",
  "repository": {
    "type": "git",
    "url": "https://github.com/commandoperator/cmdop-sdk-js.git",
    "directory": "node"
  },
  "bugs": {
    "url": "https://github.com/commandoperator/cmdop-sdk-js/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run proto:gen && tsup",
    "dev": "tsup --watch",
    "proto:gen": "buf generate src/proto/protobuf --path src/proto/protobuf/service.proto --path src/proto/protobuf/tunnel.proto",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest run",
    "clean": "rm -rf dist src/proto/generated",
    "example:terminal": "tsx examples/local/terminal.ts",
    "example:files": "tsx examples/local/files.ts",
    "example:agent": "tsx examples/local/agent.ts",
    "example:extract": "tsx examples/local/extract.ts",
    "example:all": "tsx examples/local/test-all.ts"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.10.2",
    "@cmdop/core": "workspace:*",
    "@grpc/grpc-js": "^1.12.0",
    "long": "^5.2.0",
    "nice-grpc": "^2.1.0",
    "nice-grpc-common": "^2.0.2",
    "zod": "^4"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.62.1",
    "@cmdop/eslint-config": "workspace:*",
    "@cmdop/typescript-config": "workspace:*",
    "@types/node": "^20.0.0",
    "ts-proto": "^2.6.0",
    "tsup": "^8.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "cmdop",
    "sdk",
    "node",
    "grpc",
    "terminal",
    "agent"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
